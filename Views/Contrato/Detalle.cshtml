@model Tp_Inmobiliaria_Ledesma_Lillo.Models.Contrato;
@{
    ViewData["Title"] = "Detalles";
}
<h1>Detalles</h1>
<div>
    <h4>Contrato</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdContrato)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdContrato)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.InmuebleId)
        </dt>
        <dd class="col-sm-10">
            @ViewBag.Propietario
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inquilino)
        </dt>
        <dd class="col-sm-10">
            @ViewBag.Inquilino
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FecInicio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FecInicio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FecFin)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FecFin)
        </dd>
        
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Monto)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Monto)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Editar", new { id = Model.IdContrato}) |
    <a asp-action="Listado">Volver al Listado</a>
</div>