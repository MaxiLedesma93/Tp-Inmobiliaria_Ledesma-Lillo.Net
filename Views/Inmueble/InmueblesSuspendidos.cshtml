@model IEnumerable<Tp_Inmobiliaria_Ledesma_Lillo.Models.Inmueble>
@{
    ViewData["Title"] = "Inmueble";
}   
<div>
	<h1>Inmuebles Suspendidos</h1>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ambientes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PropietarioId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Importe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Disponible)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody class="table-group-divider">
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ambientes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Uso)
            </td>
            <td>
               @(item.TipoInmueble.Descripcion)
            </td>
            <td>
               @(item.Duenio.Nombre + " " + item.Duenio.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Importe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Disp)
            </td>
            <td>
                <a asp-action="Editar" asp-route-id="@item.IdInmueble" title="Editar" class="btn btn-primary"><i class="fa-solid fa-square-pen"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>
<div>
    <a asp-action="Listado">Volver al Listado</a>
</div>