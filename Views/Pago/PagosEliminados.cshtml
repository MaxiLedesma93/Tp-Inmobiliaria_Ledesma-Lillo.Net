@model IEnumerable<Tp_Inmobiliaria_Ledesma_Lillo.Models.Pago>
@{
    ViewData["Title"] = "Pagos Eliminados";
}
<div>
<h1>Listado de Pagos Eliminados</h1>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumPago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Importe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaPago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Detalle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContratoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.contrato.Inquilino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Activo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody class="table-group-divider">
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumPago)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Importe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaPago)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Detalle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContratoId)
            </td>
            <td>
                 @(item.contrato.Inquilino.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Activo)
            </td>
        </tr>
}
    </tbody>
</table>
</div>
<div>
    <a asp-action="Listado">Volver al Listado</a>
</div>